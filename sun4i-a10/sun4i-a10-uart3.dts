/dts-v1/ /plugin/;

/ {
	compatible = "allwinner,sun4i-a10";

	fragment@0 {
		target-path = "/aliases";
		__overlay__ {
			uart3 = "/soc@01c00000/serial@01c28c00";
		};
	};

	fragment@1 {
		target = <&pio>;
		__overlay__ {
			uart3_pins_a: uart3@0 {
				pins = "PG6", "PG7", "PG8", "PG9";
				function = "uart3";
			};

			uart3_pins_a_2: uart3@1 {
				pins = "PG6", "PG7";
				function = "uart3";
			};
		};
	};

	fragment@2 {
		target = <&uart3>;
		 __overlay__ {
			pinctrl-names = "default";
			pinctrl-0 = <&uart3_pins_a_2>;
			status = "okay";
		};
	};
};
