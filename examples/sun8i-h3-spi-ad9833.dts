/dts-v1/;
/plugin/;

/ {
	compatible = "allwinner,sun8i-h3";

	fragment@0 {
		target-path = "/aliases";
		__overlay__ {
			spi0 = "/soc/spi@01c68000";
		};
	};

	fragment@1 {
		target-path = "/clocks";
		__overlay__ {
			#address-cells = <1>;
			#size-cells = <1>;
			ad983x_osc_fixed: ad983x_osc_fixed {
				compatible = "fixed-clock";
				#clock-cells = <0>;
				clock-frequency  = <25000000>;
			};
		};
	};

	fragment@2 {
		target = <&spi0>;
		__overlay__ {
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";
			ad9833@0 {
				reg = <0>; /* Chip Select 0 */
				compatible = "adi,ad9833";
				spi-max-frequency = <10000000>;
				clocks = <&ad983x_osc_fixed>;
				status = "okay";
			};

			mcp41010@1 {
				reg = <1>; /* Chip Select 1 */
				compatible = "spidev";
				spi-max-frequency = <500000>;
				status = "okay";
			};
		};
	};
};
